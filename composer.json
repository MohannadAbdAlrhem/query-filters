{
    "name": "mouhannad/query-filters",
    "description": "Laravel Package To Easily Filter Eloquent Queries",
    "type": "library",
    "keywords": [
        "mouhannad",
        "laravel",
        "query-filters",
        "filters",
        "eloquent"
    ],
    "homepage": "https://github.com/MohannadAbdAlrhem/query-filters",
    "license": "MIT",
    "authors": [
        {
            "name": "MouhannadAbdAlrhem",
            "email": "mouhannadabdalrhem@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0"
    },  
    "autoload": {
        "psr-4": {
            "Mouhannad\\QueryFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mouhannad\\QueryFilters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mouhannad\\QueryFilters\\QueryFiltersServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
